local targetPetName = "Octopus"

for _, obj in ipairs(game:GetDescendants()) do
    if (obj:IsA("MeshPart") or obj:IsA("SpecialMesh") or obj:IsA("Decal")) then
        local parentName = obj.Parent and obj.Parent.Name or ""
        if obj.Name:find(targetPetName) or parentName:find(targetPetName) then
            print("Found pet-related object:")
            print("  Name: ", obj:GetFullName())
            if obj:IsA("MeshPart") or obj:IsA("SpecialMesh") then
                print("  MeshId: ", obj.MeshId)
                print("  TextureId: ", obj.TextureId)
            elseif obj:IsA("Decal") then
                print("  Texture: ", obj.Texture)
            end
            print("-----")
        end
    end
end
