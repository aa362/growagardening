local Players = game:GetService("Players")
local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/DeltaGay/femboy/refs/heads/main/GardenSpawner.lua"))()

local player = Players.LocalPlayer
local ChatService = game:GetService("Chat")

-- Spawnerをロード
Spawner.Load()

-- チャットメッセージを監視してコマンドを実行
player.Chatted:Connect(function(msg)
    if msg:lower() == "!spawnoctopus" then
        -- オクトパスをスポーン（体重1、年齢1で例）
        Spawner.SpawnPet("Octopus", 1, 1)
        -- チャットに通知
        ChatService:Chat(player.Character.Head, "オクトパスをスポーンしました！", Enum.ChatColor.Blue)
    end
end)
