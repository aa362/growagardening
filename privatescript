local searchNames = {"Octopus", "OctopusModel", "Pet_Octopus"}

local function searchModel()
    local placesToCheck = {
        workspace,
        game:GetService("ReplicatedStorage"),
        game:GetService("ServerStorage"),
        game:GetService("StarterPlayer"),
    }

    for _, place in ipairs(placesToCheck) do
        for _, name in ipairs(searchNames) do
            local found = place:FindFirstChild(name, true) -- trueで再帰検索
            if found then
                print("Found model:", found:GetFullName())
                return found
            end
        end
    end
    return nil
end

local foundModel = searchModel()
if foundModel then
    print("モデル発見:", foundModel:GetFullName())
else
    print("モデルは見つかりませんでした。")
end
