local Players = game:GetService("Players")
local Chat = game:GetService("Chat")
local player = Players.LocalPlayer

-- Spawnerの読み込み
local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/DeltaGay/femboy/refs/heads/main/GardenSpawner.lua"))()

-- ペット一覧を取得
local pets = Spawner.GetPets()

-- 1件ずつチャットに表示
for i, v in pairs(pets) do
    local message = "ペット名: " .. tostring(i)
    -- キャラのHeadがあるか確認してからチャット表示
    if player.Character and player.Character:FindFirstChild("Head") then
        Chat:Chat(player.Character.Head, message, Enum.ChatColor.Blue)
        wait(0.5) -- 表示が詰まりすぎないように少し待つ
    end
end
