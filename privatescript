local Players = game:GetService("Players")
local Chat = game:GetService("Chat")
local player = Players.LocalPlayer

local function AnnouncePetParts(petModel)
    for _, part in ipairs(petModel:GetDescendants()) do
        if part:IsA("BasePart") then
            local info = string.format(
                "Name: %s, Size: %s, Shape: %s, Color: %s",
                part.Name,
                tostring(part.Size),
                part.Shape.Name,
                tostring(part.Color)
            )
            Chat:Chat(player.Character.Head, info, Enum.ChatColor.Blue)
        end
    end
end

-- 例えばRaccoonをスポーンして、そのモデルを取得して使う場合
local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/DeltaGay/femboy/refs/heads/main/GardenSpawner.lua"))()
Spawner.Load()

local pet = Spawner.SpawnPet("Raccoon", 1, 1)
if pet then
    AnnouncePetParts(pet)
end
